<p id="notice"><%= notice %></p>

<h1>Listing Elementos</h1>

<table>
  <thead>
    <tr>
      <th>Tipoelemento</th>
      <th>Tema</th>
      <th>Elemento descripcion</th>
      <th>Elemento fechainicio</th>
      <th>Elemento fechafinal</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @elementos.each do |elemento| %>
      <tr>
        <td><%= elemento.tipoElemento_id %></td>
        <td><%= elemento.tema_id %></td>
        <td><%= elemento.elemento_descripcion %></td>
        <td><%= elemento.elemento_fechaInicio %></td>
        <td><%= elemento.elemento_fechaFinal %></td>
        <td><%= link_to 'Show', elemento %></td>
        <td><%= link_to 'Edit', edit_elemento_path(elemento) %></td>
        <td><%= link_to 'Destroy', elemento, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Elemento', new_elemento_path %>
