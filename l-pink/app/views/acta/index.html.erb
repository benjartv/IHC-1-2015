<p id="notice"><%= notice %></p>
<h1>Listado de Actas</h1>

<table>
  <thead>
    <tr>
      <th>Proyecto</th>
      <th>Acta fecha</th>
      <th>Acta correlativo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @acta.each do |actum| %>
      <tr>
        <td><%= actum.proyecto_id %></td>
        <td><%= actum.acta_fecha %></td>
        <td><%= actum.acta_correlativo %></td>
        <td><%= link_to 'Edit', edit_actum_path(actum) %></td>
        <td><%= link_to 'Mostrar', {:controller => 'temas', :action => 'index', :parametro => actum.proyecto_id, :otroparametro => actum.id}%></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nueva Acta dialogica', new_actum_path %>
<%= link_to 'Volver a mis Proyectos', 'proyectos#index' %>